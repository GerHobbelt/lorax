{
  "name": "lorax",
  "version": "2.1.0",
  "description": "A node module which reads the git log and create a human readable changelog",
  "main": "lib/lorax.js",
  "bin": "bin/lorax",
  "scripts": {
    "prepublish": "grunt babel",
    "test": "npm run-script flow && grunt ci",
    "flow": "sh -c 'node_modules/.bin/flow; test $? -eq 0 -o $? -eq 2'",
    "build": "grunt babel && npm run flow && grunt",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianlee44/lorax.git"
  },
  "keywords": [
    "log",
    "changelog",
    "generator",
    "git",
    "markdown"
  ],
  "author": {
    "name": "Adrian Lee",
    "email": "adrian@adrianlee.me",
    "url": "http://www.adrianlee.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrianlee44/lorax/issues"
  },
  "homepage": "https://github.com/adrianlee44/lorax",
  "devDependencies": {
    "ava": "0.22.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "coveralls": "3.0.0",
    "eslint-plugin-flowtype": "2.37.0",
    "flow-bin": "0.56.0",
    "grunt": "1.0.1",
    "grunt-ava": "0.18.0",
    "grunt-babel": "7.0.0",
    "grunt-eslint": "20.1.0",
    "matchdep": "2.0.0",
    "nyc": "11.2.1"
  },
  "dependencies": {
    "commander": "2.11.0",
    "findup-sync": "2.0.0",
    "q": "1.5.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
