{
  "name": "lorax",
  "version": "2.1.0",
  "description": "A node module which reads the git log and create a human readable changelog",
  "main": "lib/lorax.js",
  "bin": "bin/lorax",
  "files": [
    "bin/lorax",
    "build/**/*"
  ],
  "scripts": {
    "prepublish": "grunt babel",
    "test": "npm run-script flow; grunt ci;",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianlee44/lorax.git"
  },
  "keywords": [
    "log",
    "changelog",
    "generator",
    "git",
    "markdown"
  ],
  "author": {
    "name": "Adrian Lee",
    "email": "adrian@adrianlee.me",
    "url": "http://www.adrianlee.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrianlee44/lorax/issues"
  },
  "homepage": "https://github.com/adrianlee44/lorax",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "ava": "^2.1.0",
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-plugin-flowtype": "^3.11.1",
    "flow-bin": "^0.102.0",
    "grunt": "^1.0.4",
    "grunt-ava": "^0.19.0",
    "grunt-babel": "^8.0.0",
    "grunt-eslint": "^22.0.0",
    "matchdep": "^2.0.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "commander": "2.20.0",
    "findup-sync": "^3.0.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
