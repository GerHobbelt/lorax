{
  "name": "lorax",
  "version": "2.1.0",
  "description": "A node module which reads the git log and create a human readable changelog",
  "main": "lib/lorax.js",
  "bin": "bin/lorax",
  "scripts": {
    "prepublish": "grunt babel",
    "test": "npm run-script flow && grunt ci",
    "flow": "sh -c 'node_modules/.bin/flow; test $? -eq 0 -o $? -eq 2'",
    "build": "grunt babel && npm run flow && grunt",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianlee44/lorax.git"
  },
  "keywords": [
    "log",
    "changelog",
    "generator",
    "git",
    "markdown"
  ],
  "author": {
    "name": "Adrian Lee",
    "email": "adrian@adrianlee.me",
    "url": "http://www.adrianlee.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrianlee44/lorax/issues"
  },
  "homepage": "https://github.com/adrianlee44/lorax",
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.6",
    "eslint-plugin-flowtype": "^2.25.0",
    "flow-bin": "^0.45.0",
    "grunt": "^1.0.1",
    "grunt-ava": "^0.17.0",
    "grunt-babel": "^6.0.0",
    "grunt-eslint": "^19.0.0",
    "matchdep": "^1.0.0",
    "nyc": "^10.3.0"
  },
  "dependencies": {
    "commander": "2.9.0",
    "findup-sync": "^1.0.0",
    "q": "1.5.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
